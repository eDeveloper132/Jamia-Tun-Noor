{
  "version": 2,
  "name": "talabaoasatiza",
  "builds": [
    {
      "src": "src/index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "public/**"
        ]
    }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.mjs"
    }
  ]
}
