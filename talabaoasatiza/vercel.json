{
  "version": 2,
  "builds": [
    {
      "src": "src/index.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "public/auth/**",
          "public/protected/**",
          "src/config/**",
          "src/controllers/**",
          "src/models/**",
          "src/routes/**",
          "src/utils/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.mjs"
    }
  ]
}