{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.mjs"
    },
    {
      "src": "^/(login|signup|logout|forgot-password|verify-email|verify-email-two)$",
      "dest": "dist/index.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.mjs"
    }
  ]
}